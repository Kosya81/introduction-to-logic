    feedback = widgets.Output()
    answer_output = widgets.Output()  # –ó–¥–µ—Å—å –ø–æ—è–≤–∏—Ç—Å—è –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π –æ—Ç–≤–µ—Ç

    def check_answer(change):
        with feedback:
            clear_output()
            user_answer = change['new'] == "–ò—Å—Ç–∏–Ω–∞"
            if user_answer == correct:
                display(HTML("<span style='color: green;'>‚úîÔ∏è –í–µ—Ä–Ω–æ!</span>"))
            else:
                display(HTML("<span style='color: red;'>‚ùå –ù–µ–≤–µ—Ä–Ω–æ.</span>"))

    buttons.observe(check_answer, names='value')
    
    container = widgets.VBox([label, buttons, feedback, answer_output])
    question_widgets.append((correct, answer_output))
    display(container)
    display(HTML("<hr>"))

