def create_interactive_question(text, correct):
    label = widgets.HTML(value=f"<b>{text}</b>")
    buttons = widgets.ToggleButtons(
        options=["–ò—Å—Ç–∏–Ω–∞", "–õ–æ–∂—å"],
        description="–í–∞—à –æ—Ç–≤–µ—Ç:",
        button_style=''
